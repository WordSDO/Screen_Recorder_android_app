<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt">
    <item>
        <shape android:shape="oval">
            <solid android:color="@color/colorRed_trans"/>
            <size android:height="192dp" android:width="192dp"/>
        </shape>
    </item>
    <item android:left="1dp" android:right="1dp" android:bottom="2dp">
        <shape android:shape="oval">
            <solid android:color="@color/colorRed_trans"/>
            <corners android:radius="5dp"/>
        </shape>
    </item>
</layer-list>
